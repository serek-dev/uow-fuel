{
  "name": "stwarog/uow-fuel",
  "description": "Unit of Work feature for Fuel's PHP ORM.",
  "type": "library",
  "require": {
    "php": ">=7.1",
    "fuel/core": "^1.8",
    "fuel/orm": "^1.8",
    "stwarog/uow": "^1.1"
  },
  "require-dev": {
    "symfony/var-dumper": "^5.1",
    "phpunit/phpunit": "7.1.3"
  },
  "autoload": {
    "psr-4": {
      "Stwarog\\UowFuel\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      {
        "Tests\\": "tests/"
      }
    ]
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Sebastian Twarog",
      "email": "contact@stwarog.com"
    }
  ],
  "extra": {
    "installer-paths": {
      "vendor/{$name}/": [
        "type:fuel-package"
      ]
    }
  },
  "minimum-stability": "stable"
}
